<p id="notice"><%= notice %></p>

<h1>Userinfors</h1>

<table>
  <thead>
    <tr>
      <th>First name</th>
      <th>Last name</th>
      <th>Photo</th>
      <th>Birth day</th>
      <th>Phone</th>
      <th>Gender</th>
      <th>Address</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @userinfors.each do |userinfor| %>
      <tr>
        <%if userinfor.user == current_user%>

        <td><%= userinfor.first_name %></td>
        <td><%= userinfor.last_name %></td>
        <td><%= userinfor.photo %></td>
        <td><%= userinfor.birth_day %></td>
        <td><%= userinfor.phone %></td>
        <td><%= userinfor.gender %></td>
        <td><%= userinfor.address %></td>
        <td><%= link_to 'Show', userinfor %></td>
        <td><%= link_to 'Edit', edit_userinfor_path(userinfor) %></td>
        <td><%= link_to 'Destroy', userinfor, method: :delete, data: { confirm: 'Are you sure?' } %></td>
            <%end%>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Userinfor', new_userinfor_path %>
